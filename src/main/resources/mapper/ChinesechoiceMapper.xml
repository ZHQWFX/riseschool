<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhq.mapper.ChinesechoiceMapper">
    <insert id="addchinesechoice"  parameterType="Chinesechoice">
        insert into chinesechoice values(null,#{question},#{optionA},#{optionB},#{optionC},#{optionD},#{answer},#{type},#{analysis})
    </insert>
    <select id="allchinesechoice" resultType="Chinesechoice" parameterType="String">
        select * from chinesechoice where type = #{type}
    </select>
</mapper>