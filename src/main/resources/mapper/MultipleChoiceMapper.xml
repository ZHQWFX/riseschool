<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhq.mapper.MultipleChoiceMapper">
    <!-- 查找出所有的多选题
     List<MultipleChoice> allmultiplechoice();-->
    <select id="allmultiplechoice" resultType="MultipleChoice">
        select * from multiple_choice where subject = #{subject}
    </select>
</mapper>




